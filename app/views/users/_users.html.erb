<% if users.any? %>
  <ul class="list-unstyled">
    <% users.each do |user| %>
        <li class="media">
            <div>
                <%= icon_url(user) %>
            </div>
            <div class="media-body">
                <div>
                    <p><%= link_to user.name, user_path(user) %></p>
                </div>
            </div>
        </li>
    <% end %>
  </ul>
  <%= paginate users %>
<% end %>